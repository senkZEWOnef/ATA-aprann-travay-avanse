import json
import requests
import os
import math
from datetime import datetime

class PersonalAssistant:
    """
    ü§ñ Personal Assistant CLI Application
    Combines all Python concepts learned in the course
    """
    
    def __init__(self):
        self.notes_file = "notes.json"
        self.settings_file = "settings.json"
        self.load_data()
    
    def load_data(self):
        """Load notes and settings from files"""
        try:
            with open(self.notes_file, 'r') as f:
                self.notes = json.load(f)
        except FileNotFoundError:
            self.notes = []
        
        try:
            with open(self.settings_file, 'r') as f:
                self.settings = json.load(f)
        except FileNotFoundError:
            self.settings = {"default_city": "Port-au-Prince", "theme": "dark"}
    
    def save_data(self):
        """Save notes and settings to files"""
        with open(self.notes_file, 'w') as f:
            json.dump(self.notes, f, indent=2)
        
        with open(self.settings_file, 'w') as f:
            json.dump(self.settings, f, indent=2)
    
    def display_menu(self):
        """Display the main menu"""
        print("\\n" + "="*50)
        print("ü§ñ PERSONAL ASSISTANT")
        print("="*50)
        print("1. üìù Manage Notes")
        print("2. üå§Ô∏è  Check Weather")
        print("3. üßÆ Calculator")
        print("4. üìä Data Manager") 
        print("5. ‚öôÔ∏è  Settings")
        print("6. üö™ Exit")
        print("-"*50)
    
    def manage_notes(self):
        """Note management submenu"""
        while True:
            print("\\nüìù NOTE MANAGER")
            print("1. Add Note")
            print("2. View All Notes")
            print("3. Search Notes")
            print("4. Delete Note")
            print("5. Back to Main Menu")
            
            choice = input("Choose option: ").strip()
            
            if choice == "1":
                self.add_note()
            elif choice == "2":
                self.view_notes()
            elif choice == "3":
                self.search_notes()
            elif choice == "4":
                self.delete_note()
            elif choice == "5":
                break
    
    def add_note(self):
        """Add a new note"""
        note_text = input("Enter your note: ").strip()
        if note_text:
            note = {
                "id": len(self.notes) + 1,
                "text": note_text,
                "date": datetime.now().strftime("%Y-%m-%d %H:%M:%S")
            }
            self.notes.append(note)
            print("‚úÖ Note added successfully!")
    
    def view_notes(self):
        """View all notes"""
        if not self.notes:
            print("üì≠ No notes found.")
            return
        
        print("\\nüìã YOUR NOTES:")
        for note in self.notes:
            print(f"{note['id']}. [{note['date']}] {note['text']}")
    
    def search_notes(self):
        """Search notes by keyword"""
        if not self.notes:
            print("üì≠ No notes to search.")
            return
            
        keyword = input("Enter search keyword: ").lower()
        found_notes = [note for note in self.notes if keyword in note['text'].lower()]
        
        if found_notes:
            print(f"\\nüîç Found {len(found_notes)} notes:")
            for note in found_notes:
                print(f"{note['id']}. [{note['date']}] {note['text']}")
        else:
            print("üîç No notes found with that keyword.")
    
    def delete_note(self):
        """Delete a note by ID"""
        if not self.notes:
            print("üì≠ No notes to delete.")
            return
        
        self.view_notes()
        try:
            note_id = int(input("Enter note ID to delete: "))
            self.notes = [note for note in self.notes if note['id'] != note_id]
            print("üóëÔ∏è Note deleted successfully!")
        except (ValueError, IndexError):
            print("‚ùå Invalid note ID.")
    
    def check_weather(self):
        """Get weather information"""
        city = input(f"Enter city name (or press Enter for {self.settings['default_city']}): ").strip()
        if not city:
            city = self.settings['default_city']
        
        try:
            url = f"https://wttr.in/{city}?format=j1"
            response = requests.get(url, timeout=5)
            
            if response.status_code == 200:
                data = response.json()
                current = data["current_condition"][0]
                
                print(f"\\nüå§Ô∏è Weather in {city.title()}:")
                print(f"Temperature: {current['temp_C']}¬∞C (feels like {current['FeelsLikeC']}¬∞C)")
                print(f"Condition: {current['weatherDesc'][0]['value']}")
                print(f"Humidity: {current['humidity']}%")
                print(f"Wind: {current['windspeedKmph']} km/h")
            else:
                print("‚ùå Could not fetch weather data. Please check city name.")
        
        except requests.RequestException:
            print("‚ùå Network error. Please check your internet connection.")
        except KeyError:
            print("‚ùå Error parsing weather data.")
    
    def calculator(self):
        """Basic calculator functionality"""
        print("\\nüßÆ CALCULATOR")
        print("Available operations: +, -, *, /, **, sqrt, log")
        
        try:
            expression = input("Enter calculation (e.g., '2 + 3' or 'sqrt(16)'): ").strip()
            
            if "sqrt(" in expression:
                # Handle square root
                number = float(expression.replace("sqrt(", "").replace(")", ""))
                result = math.sqrt(number)
                print(f"Result: {result}")
            elif "log(" in expression:
                # Handle logarithm
                number = float(expression.replace("log(", "").replace(")", ""))
                result = math.log(number)
                print(f"Result: {result}")
            else:
                # Handle basic operations
                result = eval(expression)
                print(f"Result: {result}")
        
        except (ValueError, SyntaxError, NameError):
            print("‚ùå Invalid expression. Please try again.")
        except ZeroDivisionError:
            print("‚ùå Cannot divide by zero.")
    
    def data_manager(self):
        """Export/import data"""
        print("\\nüìä DATA MANAGER")
        print("1. Export all data")
        print("2. Import data")
        print("3. View data statistics")
        print("4. Back to main menu")
        
        choice = input("Choose option: ").strip()
        
        if choice == "1":
            self.export_data()
        elif choice == "2":
            self.import_data()
        elif choice == "3":
            self.data_statistics()
    
    def export_data(self):
        """Export all data to a backup file"""
        backup_data = {
            "notes": self.notes,
            "settings": self.settings,
            "export_date": datetime.now().isoformat()
        }
        
        filename = f"backup_{datetime.now().strftime('%Y%m%d_%H%M%S')}.json"
        with open(filename, 'w') as f:
            json.dump(backup_data, f, indent=2)
        
        print(f"‚úÖ Data exported to {filename}")
    
    def import_data(self):
        """Import data from a backup file"""
        filename = input("Enter backup filename: ").strip()
        try:
            with open(filename, 'r') as f:
                backup_data = json.load(f)
            
            self.notes = backup_data.get("notes", [])
            self.settings = backup_data.get("settings", {})
            print("‚úÖ Data imported successfully!")
        
        except FileNotFoundError:
            print("‚ùå Backup file not found.")
        except json.JSONDecodeError:
            print("‚ùå Invalid backup file format.")
    
    def data_statistics(self):
        """Show data statistics"""
        print(f"\\nüìä DATA STATISTICS:")
        print(f"Total notes: {len(self.notes)}")
        
        if self.notes:
            latest_note = max(self.notes, key=lambda x: x['date'])
            print(f"Latest note: {latest_note['date']}")
        
        print(f"Settings configured: {len(self.settings)}")
    
    def settings(self):
        """Manage application settings"""
        print("\\n‚öôÔ∏è SETTINGS")
        print("1. Change default city")
        print("2. View current settings")
        print("3. Reset settings")
        print("4. Back to main menu")
        
        choice = input("Choose option: ").strip()
        
        if choice == "1":
            new_city = input("Enter new default city: ").strip()
            if new_city:
                self.settings["default_city"] = new_city
                print("‚úÖ Default city updated!")
        
        elif choice == "2":
            print("\\nCurrent settings:")
            for key, value in self.settings.items():
                print(f"  {key}: {value}")
        
        elif choice == "3":
            self.settings = {"default_city": "Port-au-Prince", "theme": "dark"}
            print("‚úÖ Settings reset to defaults!")
    
    def run(self):
        """Main application loop"""
        print("üéâ Welcome to your Personal Assistant!")
        
        while True:
            try:
                self.display_menu()
                choice = input("Choose an option (1-6): ").strip()
                
                if choice == "1":
                    self.manage_notes()
                elif choice == "2":
                    self.check_weather()
                elif choice == "3":
                    self.calculator()
                elif choice == "4":
                    self.data_manager()
                elif choice == "5":
                    self.settings()
                elif choice == "6":
                    print("\\nüëã Goodbye! Thanks for using Personal Assistant!")
                    self.save_data()
                    break
                else:
                    print("‚ùå Invalid option. Please choose 1-6.")
            
            except KeyboardInterrupt:
                print("\\n\\nüëã Goodbye!")
                self.save_data()
                break
            except Exception as e:
                print(f"‚ùå An error occurred: {e}")
                print("Please try again or contact support.")

